{
  "name": "@tradle/embed",
  "version": "1.4.2",
  "description": "utils for replacing, extracting and resolving media embedded in tradle objects",
  "main": "./cjs/index.js",
  "types": "./cjs/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./cjs/index.d.ts",
      "require": "./cjs/index.js"
    },
    "./types": {
      "types": "./cjs/types.d.ts",
      "require": "./cjs/types.js"
    }
  },
  "keywords": [],
  "author": "Mark Vayngrib <mark@tradle.io> (http://github.com/mvayngrib)",
  "license": "MIT",
  "ts-standard": {
    "ignore": [
      "./cjs/*"
    ]
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.cjs.json",
    "prepare": "npm run lint && npm run build && npm run coverage",
    "test": "npm run lint && npm run unit",
    "lint": "ts-standard",
    "coverage": "c8 npm run unit",
    "unit": "ts-node test/index"
  },
  "dependencies": {
    "@types/strong-data-uri": "^1.0.0",
    "@types/node": "*",
    "caseless": "^0.12.0",
    "dot-prop": "^6.0.1",
    "ip": "^1.1.5",
    "p-map": "^4.0.0",
    "strong-data-uri": "^1.0.6",
    "traverse": "^0.6.6"
  },
  "devDependencies": {
    "@types/caseless": "^0.12.2",
    "@types/ip": "^1.1.0",
    "@types/node": "^10.17.9",
    "@types/traverse": "^0.6.32",
    "c8": "^7.11.0",
    "fresh-tape": "^5.5.0",
    "ts-node": "^10.5.0",
    "ts-standard": "^11.0.0",
    "typescript": "^4.4.4"
  }
}
